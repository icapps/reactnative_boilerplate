{
  "name": "reactnative_boilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0-beta",
    "babel-core": "^7.0.0-beta",
    "babel-eslint": "8.2.6",
    "babel-preset-react-native": "^5.0.1",
    "babel-runtime": "^7.0.0-beta",
    "enzyme": "3.3.0",
    "eslint": "v4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-react-native": "3.2.1",
    "jest": "^23.4.1",
    "react-test-renderer": "16.4.1"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=debug --appIdSuffix=debug",
    "ios": "react-native run-ios",
    "lint": "eslint -c .eslintrc --ext .js src",
    "lint:fix": "eslint -c .eslintrc --ext .js src --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "validate": "yarn lint && yarn test"
  },
  "jest": {
    "automock": false,
    "preset": "react-native",
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-native": "~0.56.0",
    "react-native-config": "^0.11.5",
    "react-native-device-info": "^0.22.0",
    "react-native-i18n": "^2.0.15",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  }
}
