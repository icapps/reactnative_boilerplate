{
  "name": "reactnative_boilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.0.0-beta",
    "@babel/runtime": "7.0.0-beta.54",
    "@storybook/addon-actions": "^4.0.0-alpha.14",
    "@storybook/addon-links": "^4.0.0-alpha.14",
    "@storybook/addons": "^4.0.0-alpha.14",
    "@storybook/react-native": "^4.0.0-alpha.14",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0-beta.3",
    "babel-preset-react-native": "5.0.2",
    "babel-runtime": "^7.0.0-beta.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "v4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-react-native": "3.2.1",
    "jest": "^23.4.1",
    "jest-react-native": "^18.0.0",
    "react-dom": "^16.4.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.4.1",
    "storybook-addon-jsx": "^5.4.0"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android --variant=debug --appIdSuffix=debug",
    "ios": "react-native run-ios",
    "lint": "eslint -c .eslintrc --ext .js src",
    "lint:fix": "eslint -c .eslintrc --ext .js src --fix",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "validate": "yarn lint && yarn test"
  },
  "jest": {
    "automock": false,
    "preset": "react-native",
    "setupFiles": [
      "./setupJest.js"
    ],
    "transform": {
      "^.+\\.js$": "./node_modules/react-native/jest/preprocessor.js"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-native": "~0.56.0",
    "react-native-config": "^0.11.5",
    "react-native-device-info": "^0.22.0",
    "react-native-i18n": "^2.0.15",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.story.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
